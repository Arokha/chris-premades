{
  "id": "chris-premades",
  "title": "Chris's Premades",
  "description": "Chris's premade items for high-automation midi-qol games.",
  "version": "#{VERSION}#",
  "library": "false",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": 10,
    "verified": 10,
    "maximum": 11
  },
  "authors": [
    {
      "name": "Chris",
      "url": "https://github.com/chrisk123999/chris-premades",
      "discord": "Chris#8375",
	  "ko-fi": "chrisk123999"
    }
  ],
  "relationships": {
    "systems": [{
		"id": "dnd5e",
		"type": "system",
		"manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/master/system.json",
		"compatibility": {
			"verified": "2.1.4"
		}
	}],
    "requires": [{
		"id": "midi-qol",
		"type": "module",
		"manifest": "https://gitlab.com/tposney/midi-qol/raw/v10/package/module.json",
		"compatibility": {
			"verified": "10.0.29"
		}
	},
	{
		"id": "warpgate",
		"type": "module",
		"manifest": "https://github.com/trioderegion/warpgate/blob/master/module.json",
		"compatibility": {
			"verified": "1.16.0"
		}
	},
	{
		"id": "effectmacro",
		"type": "module",
		"manifest": "https://github.com/krbz999/effectmacro/blob/main/module.json",
		"compatibility": {
			"verified": "10.0.15"
		}
	},
	{
		"id": "templatemacro",
		"type": "module",
		"manifest": "https://github.com/krbz999/templatemacro/blob/main/module.json",
		"compatibility": {
			"verified": "10.0.1"
		}
	}]
  },
  "esmodules": [
    "scripts/module.js"
  ],
  "styles": [
    "styles/module.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md",
  "media": [
    {
      "type": "icon",
      "url": ""
    }
  ]
}
